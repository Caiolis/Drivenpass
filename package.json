{
  "name": "drivenpass",
  "version": "1.0.0",
  "description": "A password manager using typescript",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "migration:run": "prisma migrate dev",
    "start": "npm run migration:run && node dist/index.js",
    "test:load-envs": "dotenv -e .env.test",
    "test:coverage": "npm run test:load-envs -- jest -- --coverage",
    "test:migration:run": "npm run test:load-envs prisma migrate deploy",
    "test": "npm run test:load-envs jest",
    "dev": "cross-env NODE_ENV=development nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register ./src/server.ts' -e ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cryptr": "^6.3.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-status": "^1.7.3",
    "joi": "^17.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
